{
  "extends": "solium:recommended",
  "plugins": [
    "security"
  ],
  "rules": {
    "security/no-inline-assembly": [
      "off"
    ],
    "quotes": [
      "error",
      "double"
    ],
    "indentation": [
      "warning",
      4
    ],
    "linebreak-style": [
      "error",
      "unix"
    ]
  }
}
